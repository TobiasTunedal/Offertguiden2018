@model IEnumerable<Offertguiden.Models.Blog>

@{
    ViewData["Title"] = "Index";
}


<br /><br /><br />

<h2>Index</h2>

<p>
    <a asp-action="Create">Skapa nytt blogginlägg</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Title)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Date)
                </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.BlogId">Ändra</a> |
                <a asp-action="Details" asp-route-id="@item.BlogId">Se Inlägget</a> |
                <a asp-action="Delete" asp-route-id="@item.BlogId">Ta Bort</a>
            </td>
        </tr>
}
    </tbody>
</table>
<div>
    <a asp-area="" asp-controller="Home" asp-action="LoggedIn">Tillbaka</a>
</div>
Model